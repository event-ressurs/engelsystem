{% macro angel() %}
    <span class="glyphicon glyphicon-user"></span>
{% endmacro %}

{% macro glyphicon(glyph) %}
    <span class="glyphicon glyphicon-{{ glyph }}"></span>
{% endmacro %}

{% macro alert(message, type) %}
    <div class="alert alert-{{ type|default('info') }}">{{ message }}</div>
{% endmacro %}
